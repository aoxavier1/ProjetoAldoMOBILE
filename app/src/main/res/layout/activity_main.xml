<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="412dp"
        android:layout_height="74dp"
        android:layout_marginTop="92dp"
        android:onClick="irParaCalculadora"
        android:text="Calculadora Básica"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <Button
        android:id="@+id/button2"
        android:layout_width="409dp"
        android:layout_height="78dp"
        android:text="Navegador"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="102dp"
        android:onClick="irParaNavegador"/>

    <Button
        android:id="@+id/button5"
        android:layout_width="411dp"
        android:layout_height="78dp"
        android:layout_marginTop="88dp"
        android:text="Agenda telefônica"
        app:layout_constraintTop_toBottomOf="@+id/button"
        tools:layout_editor_absoluteX="0dp"
        android:onClick="irParaAgenda"/>

    <Button
        android:id="@+id/button10"
        android:layout_width="411dp"
        android:layout_height="78dp"
        android:layout_marginTop="100dp"
        android:text="Sua Localização"
        app:layout_constraintTop_toBottomOf="@+id/button5"
        tools:layout_editor_absoluteX="1dp"
        android:onClick="irParaLocalizacao"/>

</androidx.constraintlayout.widget.ConstraintLayout>